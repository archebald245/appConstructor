<?xml version="1.0"?>
<widget xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:gap="http://phonegap.com/ns/1.0" xmlns="http://www.w3.org/ns/widgets" id="com.appconstructor.housemanagement" version="0.0.1">
  <name>House management</name>
  <description>The application for managing the house and the connection between its occupants</description>
  <plugin name="cordova-plugin-inappbrowser" spec="1.5.0" source="npm" />
  <plugin name="cordova-plugin-file-transfer" spec="1.6.0" source="npm" />
  <plugin name="cordova-plugin-network-information" spec="1.3.0" source="npm" />
  <plugin name="cordova-plugin-whitelist" spec="1.3.2" source="npm" />
  <plugin name="cordova-plugin-x-toast" spec="2.5.2" source="npm" />
  <plugin name="cordova-plugin-statusbar" spec="2.3.0" source="npm" />
  <plugin name="cordova-plugin-document-viewer" spec="0.9.5" source="npm" />
  <plugin name="cordova-plugin-device" spec="2.0.1" source="npm" />
  <plugin name="cordova-plugin-file-opener2" spec="2.0.19" source="npm" />
  <plugin name="cordova-plugin-file" spec="6.0.1" source="npm" />
  <allow-navigation href="http://*/*" />
  <allow-navigation href="https://*youtube.com/*" />
  <allow-intent href="*" />
  <access origin="*" />
  <feature name="http://api.phonegap.com/1.0/file">
    <param name="" value="" />
  </feature>
  <feature name="http://api.phonegap.com/1.0/network">
    <param name="" value="" />
  </feature>
  <feature name="File">
    <param name="android-package" value="org.apache.cordova.file.FileUtils" />
  </feature>
  <feature name="Toast">
    <param name="android-package" value="nl.xservices.plugins.Toast" />
  </feature>
  <feature name="Toast">
    <param name="ios-package" value="Toast" />
  </feature>
  <feature name="Toast">
    <param name="wp-package" value="Toast" />
  </feature>
  <feature name="FileTransfer">
    <param name="android-package" value="org.apache.cordova.filetransfer.FileTransfer" />
  </feature>
  <platform name="android">
    <preference name="android-manifest/application/@android:theme" value="@android:style/Theme.NoTitleBar.Fullscreen" />
    <preference name="android-manifest/application/activity/@android:windowSoftInputMode" value="stateHidden|adjustPan" />
    <preference name="android-manifest/application/activity/@android:configChanges" value="keyboardHidden|orientation" />
    <preference name="fullscreen" value="false" />
    <preference name="DisallowOverscroll" value="true" />
    <preference name="EnableViewportScale" value="true" />
  </platform>
  <platform name="ios" />
  <preference name="show-splash-screen-spinner" value="false" />
  <preference name="auto-hide-splash-screen" value="true" />
  <preference name="SplashScreen" value="screen" />
  <preference name="SplashScreenDelay" value="3000" />
  <preference name="orientation" value="default" />
  <preference name="phonegap-version" value="cli-7.0.1" />
  <icon src="icon.png" />
